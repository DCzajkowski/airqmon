{
  "extends": [
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [true, "always-prefix"],
    "import-name": [false],
    "variable-name": {
      "options": [
        "allow-pascal-case"
      ]
    },
    "strict-boolean-expressions": [
      true,
      "allow-null-union",
      "allow-undefined-union"
    ]
  }
}
