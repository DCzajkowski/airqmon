{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Electron: Main",
          "protocol": "inspector",
          "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
          "sourceMaps": true,
          "runtimeArgs": [
              "--enable-logging",
              "."
          ],
          "windows": {
              "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
          },
          "preLaunchTask": "build"
      }
  ]
}
